#define _CRT_SECURE_NO_WARNINGS	
#include<stdio.h>
int main()
{
	int a = 0;
	int b = 0;
	int c = 0;
	for (a = 1; a <= 9; a++)
	{
		for (b = 0; b <= 9; b++)
		{
			for (c = 1; c <= 9; c++)
			{
				if ((a*100 +b*10 + c) + (c*100+ b*10 + a) == 1333)
				{
					printf("a=%d,b=%d,c=%d\n", a, b, c);
				}
			}
		}
	}
}
