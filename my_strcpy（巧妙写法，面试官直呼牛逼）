#define _CRT_SECURE_NO_WARNINGS	
#include<stdio.h>
#include<string>
#include<assert.h>
char* my_strlen(char* dest, const char* src)
{
	assert(dest != NULL);
	assert(src != NULL);
	char* ret = dest;
	while (*dest++ = *src++)
	{
		;
	}
	return ret;
}
int main()
{
	char arr1[] = "abcdefghi";
	char arr2[] = "bit";
	my_strlen(arr1, arr2);
	printf("%s\n", arr1);
	return 0;
}
